Algoritmo paras
    Dimension A[10];
    para cada elemento de A Hacer
        elemento <- azar(100);
    FinPara
    
    Escribir "Los elementos del arreglo son:";
    para i desde 1 hasta 10 Hacer
        escribir "Posición " i ": " A[i];
    FinPara
	
	Escribir ""
    
    Escribir "En orden inverso:";
    para i desde 1 hasta 10 Hacer
        escribir sin bajar A[i] " ";
    FinPara
    
FinAlgoritmo
